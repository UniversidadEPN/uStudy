<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ServiciosRealizados">



    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarServiciosRealizados"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/common_google_signin_btn_text_dark_focused"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:backgroundTint="@color/common_google_signin_btn_text_dark_focused"
        android:contentDescription="TODO"
        android:onClick="onClickAtras"
        android:tint="@color/common_google_signin_btn_text_dark_default"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarServiciosRealizados"
        app:layout_constraintStart_toStartOf="@+id/toolbarServiciosRealizados"
        app:layout_constraintTop_toTopOf="@+id/toolbarServiciosRealizados"
        app:srcCompat="?attr/homeAsUpIndicator" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="172dp"
        android:layout_height="36dp"
        android:text="@string/servicio_titulo"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton3"
        app:layout_constraintEnd_toEndOf="@+id/toolbarServiciosRealizados"
        app:layout_constraintStart_toStartOf="@+id/toolbarServiciosRealizados"
        app:layout_constraintTop_toTopOf="@+id/imageButton3" />

    <ListView
        android:id="@+id/listaAyuda"
        android:layout_width="432dp"
        android:layout_height="803dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarServiciosRealizados" />


</androidx.constraintlayout.widget.ConstraintLayout>